#include<stdio.h>
int main() {
    int num,i,flag=0;
    printf("Enter the Number: ");
    scanf("%d",&num);
    if(num<=1){
        printf("This Number is prime number.%d\n",num);
    }
    else{
        for (i=2;i<= num/2;i++){
            if(num%i==0){
                flag=1;
                break;
            }
        }
        if(flag==0)
            printf("This Number is prime number: %d\n",num);
        else
            printf("This Number is not prime number: %d\n",num);
    }

    return 0;
}
